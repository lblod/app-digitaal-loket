          form:prototype [
            form:shape [
              a schema:WebSite ;
              dct:description " "@nl;
              dct:title " "@nl;
              schema:url "";
              dct:description " "@en;
              dct:title " "@en
            ];
          ];
          form:dataGenerator form:addMuUuid.

        ##########################################################
        #  property-group: ProcedureWebsite-sub
