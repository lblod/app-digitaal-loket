            form:grouping form:Bag ;
            sh:path lblodSubsidie:timeBlock ;
            form:conceptScheme <http://lblod.data.gift/concept-schemes/a3293c46-06b7-458d-a15c-2f312294773f> ;
            sh:resultMessage "Selecteer een waarde uit de lijst."@nl
          ],
          [ a form:RequiredConstraint ;
            form:grouping form:Bag ;
            sh:resultMessage "Dit veld is verplicht."@nl;
            sh:path lblodSubsidie:timeBlock
          ] ;
    form:options  """{"conceptScheme":"http://lblod.data.gift/concept-schemes/a3293c46-06b7-458d-a15c-2f312294773f", "searchEnabled": false}""" ;
    form:displayType displayTypes:conceptSchemeSelector ;
