fields:68e149ec-1821-477b-a901-72924236ef9c a form:Field ;
    mu:uuid "68e149ec-1821-477b-a901-72924236ef9c";
    sh:name "Kies subsidiemaatregel" ;
    sh:order 20 ;
    sh:path lblodSubsidie:subsidyMeasure ;
    form:validations
          [ a form:ConceptSchemeConstraint ;
            form:grouping form:Bag ;
            sh:path lblodSubsidie:subsidyMeasure ;
            form:conceptScheme <http://lblod.data.gift/concept-schemes/33e233eb-6a9b-4141-b607-2ff72fe2ded2> ;
            sh:resultMessage "Selecteer een waarde uit de lijst."@nl
          ],
          [ a form:RequiredConstraint ;
            form:grouping form:Bag ;
            sh:resultMessage "Dit veld is verplicht."@nl;
            sh:path lblodSubsidie:subsidyMeasure
          ] ;
    form:options  """{"conceptScheme":"http://lblod.data.gift/concept-schemes/33e233eb-6a9b-4141-b607-2ff72fe2ded2", "searchEnabled": false}""" ;
    form:displayType displayTypes:conceptSchemeSelector ;
    sh:group fields:d93dfe7a-a668-4a49-a77f-981433f49ba1 .

